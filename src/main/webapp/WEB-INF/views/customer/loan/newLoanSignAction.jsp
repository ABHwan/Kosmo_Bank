		<script type="text/javascript">
			alert("신규 대출 신청 완료");
			window.location="loanAccountList.do";
		</script>
 	</c:if>
 	<c:if test="${insertCnt != 1}">
		<script type="text/javascript">
			alert("신규 대출 신청이 완료되지 않았습니다. 다시 시도하세요.");
			window.history.back();
		</script>
 	</c:if>
 	
	
